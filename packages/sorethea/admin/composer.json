{
    "name": "sorethea/admin",
    "description": "Laravel filament admin package",
    "minimum-stability": "dev",
    "type": "filament-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Sorethea Ok",
            "email": "sorethea@gmail.com"
        }
    ],
    "keywords": [
        "sorethea",
        "admin"
    ],
    "require": {
        "filament/filament": "^2.16.62",
        "filament/spatie-laravel-media-library-plugin": "^2.16.62",
        "filament/spatie-laravel-settings-plugin": "^2.16.62",
        "spatie/laravel-medialibrary": "^10.7.9",
        "spatie/laravel-permission": "^5.8.0"
    },
    "autoload": {
        "psr-4": {
            "Sorethea\\Admin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sorethea\\Admin\\AdminServiceProvider"
            ],
            "aliases": {
                "Admin": "Sorethea\\Admin\\Facades\\Admin"
            }
        }
    }
}
